{
    "executionRoleArn": "arn:aws:iam::100450219031:role/sacle-ecs-task-iam-role",
    "containerDefinitions": [
        {
            "name": "test-container",
            "image": "100450219031.dkr.ecr.ap-northeast-2.amazonaws.com/tomcat-test:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "host",
    "cpu": "256",
    "memory": "512",
    "family": "test-task"
}
